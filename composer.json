{
    "name": "osoobe/laravel-utilities",
    "description": "A description for utilities.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Oshane Bailey",
            "email": "b4.oshany@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "alariva/laravel-email-domain-blacklist": "*"
    },
    "autoload": {
        "psr-4": {
            "Osoobe\\Utilities\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osoobe\\Utilities\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osoobe\\Utilities\\UtilitiesServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
